{
  "manifest_version": 3,
  "name": "Huly Select All Issues",
  "version": "1.0",
  "description": "Loads and selects all issues in huly.app",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://huly.app/workbench/*/tracker/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "yy16.png",
    "48": "yy48.png",
    "128": "yy128.png"
  }
}
